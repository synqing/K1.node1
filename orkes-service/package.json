{
  "name": "k1-orkes-service",
  "version": "0.1.0",
  "description": "Orkes Conductor workflow orchestration service for K1.node1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "vitest",
    "register:pattern": "tsx src/scripts/register.ts pattern",
    "register:cicd": "tsx src/scripts/register.ts cicd",
    "register:all": "tsx src/scripts/register.ts all",
    "e2e": "tsx src/scripts/run-e2e.ts",
    "e2e:local": "npm run -s build && tsx src/scripts/serve-and-run.ts"
  },
  "dependencies": {
    "@io-orkes/conductor-javascript": "^2.1.5",
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.17.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
